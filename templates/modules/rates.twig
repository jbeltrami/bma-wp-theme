<section class="rates">
	<div class="container">
		<div class="row align-items-center justify-content-center">
			<div class="col-lg-8">
				{% for rate in rates %}
					<div class="row no-gutters">
						<div class="col-lg-6">
							<p>{{rate.duration}}</p>
						</div>
						<div class="col-lg-6">
							<p>{{rate.rate}}</p>
						</div>
					</div>
				{% endfor %}
			</div>
		</div>
	</div>
</section>
